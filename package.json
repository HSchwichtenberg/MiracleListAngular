{
    "name": "miracle-list-client",
    "version": "0.6.0-Beta.3",
    "date": "11.08.2017",
    "license": "MIT",
    "angular-cli": {},
    "main": "electron-main.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "lint": "tslint \"src/**/*.ts\"",
        "//": "---------------------------- Skripte für Testen",
        "test": "ng test",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "protractor",
        "///": "---------------------------- Skripte für Electron zum Entwickeln w = laufen auf windows m = laufen auf mac",
        "w-copy-electron": "xcopy electron\\* temp_electron /s /e /y",
        "m-copy-electron": "\"node_modules/.bin/copy\" \"/electron/*\" \"temp_electron\"",
        "m-build-electron": "ng build --output-path=temp_electron --base-href . && npm run m-copy-electron",
        "w-build-electron": "ng build --output-path=temp_electron --base-href . && npm run w-copy-electron",
        "m-electron": "npm run m-build-electron && electron temp_electron",
        "w-electron": "npm run w-build-electron && electron temp_electron",
        "m-electron-ohnengbuild": "npm run m-copy-electron && electron temp_electron",
        "w-electron-ohnengbuild": "npm run w-copy-electron && electron temp_electron",
        "w-electron-deploy": "npm run w-copy-electron && electron-packager temp_electron MiracleListElectron --platform=win32 --arch=x64 --out=dist_electron/ --overwrite --icon=src/assets/favicon.ico  --asar",
        "w-electron-deploy-run": "npm run w-electron-deploy && start cmd /c \"dist_electron\\MiracleListElectron-win32-x64\\MiracleListElectron.exe\"",
        "////": "---------------------------- Skripte für Cordova",
        "m-electron-deployall": "npm run m-build-electron && electron-packager temp_electron MiracleListElectron --platform=darwin,linux,win32 --arch=x64 --out=dist_electron/ --overwrite --icon=src/assets/favicon.ico  --asar",
        "w-electron-deployall": "npm run w-build-electron && electron-packager temp_electron MiracleListElectron --platform=darwin,linux,win32 --arch=x64 --out=dist_electron/ --overwrite --icon=src/assets/favicon.ico  --asar",
        "m-electron-deployallprod": "ng build -prod --output-path=temp_electron --base-href . && npm run m-copy-electron && electron-packager temp_electron MiracleListElectron --platform=darwin,linux,win32 --arch=x64 --out=dist_electron/ --overwrite --icon=src/assets/favicon.ico  --asar",
        "w-electron-deployallprod": "ng build -prod --output-path=temp_electron --base-href . && npm run w-copy-electron && electron-packager temp_electron MiracleListElectron --platform=darwin,linux,win32 --arch=x64 --out=dist_electron/ --overwrite --icon=src/assets/favicon.ico  --asar",
        "w-electron-packagelinux": "electron-packager temp_electron MiracleListElectron --platform=linux --arch=x64 --out=\\\\192.168.1.179/documents --overwrite  --asar --icon=temp_electron/icon.png",
        "build-cordova": "ng build --base-href ./ --output-path MLCordova/www/",
        "build-cordova-prod": "ng build --target=production --environment=prod --output-path MLCordova/www/",
        "c-browser": "cd MLCordova && cordova run browser",
        "c-win": "cd MLCordova && cordova run windows"
    },
    "private": true,
    "dependencies": {
        "//": {
            "@progress/kendo-angular-dialog": "^0.13.3",
            "@progress/kendo-angular-grid": "^0.13.1",
            "@progress/kendo-angular-intl": "^0.9.0",
            "@progress/kendo-data-query": "^0.3.5",
            "@progress/kendo-theme-default": "^2.12.8"
        },
        "@angular/common": "^4.3.4",
        "@angular/compiler": "^4.3.4",
        "@angular/core": "^4.3.4",
        "@angular/forms": "^4.3.4",
        "@angular/http": "^4.3.4",
        "@angular/platform-browser": "^4.3.4",
        "@angular/platform-browser-dynamic": "^4.3.4",
        "@angular/router": "^4.3.4",
        "angular-date-value-accessor": "0.0.1",
        "angular2-modal": "^2.0.2",
        "angular2-moment": "^1.1.0",
        "bootstrap": "^3.3.7",
        "bootstrap-timepicker": "^0.5.2",
        "cldr-data": "^30.0.4",
        "core-js": "^2.4.1",
        "date-input-polyfill": "^2.12.0",
        "electron-window": "^0.8.1",
        "jquery": "^3.1.1",
        "json-loader": "^0.5.4",
        "moment": "^2.17.1",
        "moo-dateinput-polyfill": "^1.5.2",
        "ng2-datetime": "^1.2.2",
        "ng2-dnd": "^2.2.2",
        "rxjs": "^5.1.0",
        "ts-helpers": "^1.1.1",
        "web-animations-js": "^2.2.2",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular/cli": "^1.3.0",
        "@angular/compiler-cli": "^4.3.4",
        "@types/electron": "^1.4.32",
        "@types/jasmine": "2.5.38",
        "@types/jquery": "^2.0.39",
        "@types/moment": "^2.13.0",
        "@types/node": "^6.0.64",
        "@types/username": "^1.0.28",
        "codelyzer": "~2.0.0",
        "copy": "^0.3.0",
        "devtron": "^1.4.0",
        "electron": "^1.4.1",
        "electron-packager": "^8.5.1",
        "jasmine-core": "~2.5.2",
        "jasmine-jquery": "2.1.1",
        "jasmine-spec-reporter": "~3.2.0",
        "karma": "~1.4.1",
        "karma-chrome-launcher": "~2.0.0",
        "karma-cli": "~1.0.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-jquery": "^0.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-coverage-istanbul-reporter": "^0.2.0",
        "protractor": "~5.1.0",
        "ts-node": "~2.0.0",
        "tslint": "~4.5.0",
        "typescript": "~2.1.0"
    }
}